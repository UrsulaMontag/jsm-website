{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "noEmit": true,
    "types": [
      "cypress"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/app/*": [
        "./src/app/*"
      ]
    }
  },
  "include": [
    "../node_modules/cypress",
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "exclude": [
    "@testing-library/jest-dom"
  ]
}